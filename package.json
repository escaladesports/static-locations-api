{
  "name": "zip-info-api",
  "version": "0.0.0",
  "keywords": [
    "zip",
    "locations",
    "coordinates",
    "latitude",
    "longitude",
    "json",
    "api"
  ],
  "main": "dist/index.js",
  "bin": {
    "zip-info-api": "dist/cli.js"
  },
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/zip-info-api.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "esca-scripts build --no-react --no-css",
    "build:api": "npm run build:json && npm run build:copy",
    "build:copy": "cp -r static/* dist-json",
    "build:json": "npm run build && node dist/cli build --path './dist-json'",
    "bundle": "esca-scripts bundle --src src/index.js --no-react --no-css --dist dist-browser",
    "dev": "esca-scripts dev --no-react --no-css",
    "prepublishOnly": "npm run build",
    "reset": "esca-scripts reset",
    "test": "npm run build && esca-scripts test --no-react --no-css",
    "travis": "opn https://travis-ci.org/escaladesports/zip-info-api/branches"
  },
  "devDependencies": {
    "esca-scripts": "^1.3.1",
    "opn-cli": "^3.1.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "fs-extra": "^6.0.1",
    "subcommander": "^1.0.0",
    "zips": "^1.1.2"
  }
}
